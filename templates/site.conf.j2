server {
{% for k,v in item.server.items() %}
{% if k.find('locations') == -1 and k != 'file_name' %}
    {{ k }} {{ v }};
{% endif %}
{% endfor %} 

{% for k,v in item.server.items() if k.find('locations') != -1 %}
    location {{ v.name }} {
{% for x,y in v.items() if x != 'name' %}       {{ x }} {{ y }};
{% endfor %}
    }
{% endfor %}
}
